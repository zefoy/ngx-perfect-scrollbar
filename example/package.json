{
  "name": "angular2-perfect-scrollbar-example",
  "license": "MIT",
  "version": "0.9.0",
  "tslint": {
    "extends": "./tslint.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "deploy": "rm -rf dist && deploy-to-git",
    "build": "webpack --config config/webpack-deploy.config.js",
    "start": "webpack-dev-server --open --inline --config config/webpack-server.config.js",
    "lint": "tslint 'src/**/*.ts' && stylelint 'src/**/*.css' --syntax css"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:zefoy/angular2-perfect-scrollbar.git",
      "branch": "gh-pages",
      "folder": "dist",
      "script": "npm run build",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "ZEF Devel",
        "email": "devel@zef.fi"
      }
    }
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "angular2-perfect-scrollbar": "0.9.0",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.0",
    "@types/node": "^6.0.0",
    "angular2-router-loader": "^0.3.0",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.0",
    "copy-webpack-plugin": "^3.0.0",
    "deploy-to-git": "0.0.7",
    "html-webpack-plugin": "^2.22.0",
    "raw-loader": "^0.5.0",
    "stylelint": "7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "tslint": "3.15.0",
    "typescript": "^2.0.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.8"
  }
}
