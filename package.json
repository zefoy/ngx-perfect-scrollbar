{
  "name": "ngx-perfect-scrollbar",
  "description": "Angular wrapper library for Perfect Scrollbar",
  "bugs": "https://github.com/zefoy/ngx-perfect-scrollbar/issues",
  "version": "9.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve app",
    "build": "ng build lib",
    "deploy": "deploy-to-git",
    "prepare": "ng build lib --prod",
    "publish": "npm publish ./dist/lib",
    "predeploy": "rimraf ./dist/app && mkdirp ./dist/app"
  },
  "tslint": {
    "extends": "./tslint.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zefoy/ngx-perfect-scrollbar.git"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:zefoy/ngx-perfect-scrollbar.git",
      "branch": "gh-pages",
      "folder": "dist/app",
      "script": "ng build app --prod -- --base-href=ngx-perfect-scrollbar --delete-output-path=false",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "ZEF Devel",
        "email": "devel@zef.fi"
      }
    }
  },
  "dependencies": {
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "core-js": "^3.6.0",
    "perfect-scrollbar": "^1.5.0",
    "resize-observer-polyfill": "^1.5.0",
    "rxjs": "^6.5.0",
    "zone.js": "^0.10.0"
  },
  "devDependencies": {
    "@angular/cli": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular-devkit/build-angular": "^0.900.0",
    "@angular-devkit/build-ng-packagr": "^0.900.0",
    "@types/node": "^12.0.0",
    "codelyzer": "^5.2.0",
    "core-js": "^3.6.0",
    "cpx": "^1.5.0",
    "deploy-to-git": "^0.1.0",
    "mkdirp": "^1.0.0",
    "ng-packagr": "^9.0.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.0",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^4.0.0",
    "terser": "^4.6.0",
    "ts-node": "^8.6.0",
    "tsickle": "^0.38.0",
    "tslint": "^6.0.0",
    "typescript": "~3.7.0",
    "watch": "^1.0.0",
    "zone.js": "^0.10.0"
  }
}
